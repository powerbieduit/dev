    {
      "parameters": {
        "jsCode": "// ðŸ§¾ Extrai os leads\nconst leads = $json._embedded?.leads || [];\n\n// ðŸ“Œ Quantidade total de leads encontrados\nconst quantidade_encontrada = leads.length;\n\n// â— Verifica se hÃ¡ leads\nif (quantidade_encontrada === 0) {\n  return [{ json: { quantidade_encontrada: 0, lead_mais_novo: null } }];\n}\n\n// âœ… Ordena por ID decrescente (mais novo primeiro)\nconst leadsOrdenados = leads\n  .filter(lead => typeof lead.id === \"number\")\n  .sort((a, b) => b.id - a.id);\n\n// ðŸ†• Lead com maior ID (mais novo)\nconst leadMaisNovo = leadsOrdenados[0];\n\n// ðŸ§© Mapeia os campos customizados para acesso fÃ¡cil\nconst campos = {};\nfor (const campo of leadMaisNovo.custom_fields_values || []) {\n  campos[campo.field_name] = campo.values?.[0]?.value || null;\n}\n\n// ðŸ“¦ Retorna a estrutura desejada\nreturn [{\n  json: {\n    quantidade_encontrada,\n    id_mais_novo: leadMaisNovo.id,\n    nome: campos[\"Nome\"] || null,\n    cpf: campos[\"CPF\"] || null,\n    ra: campos[\"RA\"] || null,\n    curso: campos[\"Curso\"] || null,\n    polo: campos[\"Polo\"] || null,\n    telefone: campos[\"Telefone\"] || null\n  }\n}];\n"
      },
