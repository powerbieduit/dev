    {
      "parameters": {
        "jsCode": "const leads = $json._embedded?.leads || [];\n\n// Se nenhum lead foi encontrado\nif (leads.length === 0) {\n  return [{\n    json: {\n      total_encontrado: 0,\n      id_mais_recente: null,\n      status_id_mais_recente: null\n    }\n  }];\n}\n\n// Ordena os leads por ID (maior ID é o mais recente)\nconst leadMaisRecente = leads.reduce((a, b) => a.id > b.id ? a : b);\n\n// Retorna a quantidade e dados do mais recente\nreturn [{\n  json: {\n    total_encontrado: leads.length,\n    id_mais_recente: leadMaisRecente.id,\n    status_id_mais_recente: leadMaisRecente.status_id || null\n  }\n}];\n"
      },
