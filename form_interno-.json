    {
      "parameters": {
        "jsCode": "const contatos = $json._embedded?.contacts || [];\n\nconst resultado = contatos.map(contato => {\n  // Busca o campo \"Phone\" nos custom_fields_values\n  let telefone = \"\";\n  const phoneField = (contato.custom_fields_values || []).find(\n    f => f.field_name === \"Phone\"\n  );\n  if (phoneField?.values?.[0]?.value) {\n    telefone = phoneField.values[0].value;\n  }\n\n  return {\n    id: contato.id,\n    name: contato.name || \"\",\n    telefone\n  };\n});\n\nreturn [{\n  json: {\n    total_encontrado: resultado.length,\n    contatos: resultado\n  }\n}];\n"
      },
