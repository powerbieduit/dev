    {
      "parameters": {
        "jsCode": "const resposta = $input.first().json.output;\nif (resposta.includes('Não Encontrado')) {\n  return [{\n    json: {\n      nome: null,\n      valor: null,\n      modalidade: null,\n      duracao: null,\n      grau: null\n    }\n  }];\n}\n// Extrai considerando quebras de linha extras e espaços\nconst linhas = resposta.split('\\n').filter(l => l.trim());\nconst dados = {};\nlinhas.forEach(linha => {\n  const [chave, ...valorArr] = linha.split(':');\n  const valor = valorArr.join(':').trim();\n  \n  if (chave.toLowerCase().includes('nome')) dados.nome = valor;\n  if (chave.toLowerCase().includes('valor')) dados.valor = valor;\n  if (chave.toLowerCase().includes('modalidade')) {\n    // Substitui Híbrido por Semipresencial\n    dados.modalidade = valor.replace(/Híbrido/gi, 'Semipresencial');\n  }\n  if (chave.toLowerCase().includes('duração') || chave.toLowerCase().includes('duracao')) dados.duracao = valor;\n  if (chave.toLowerCase().includes('grau')) {\n    // Normaliza o grau para os valores esperados\n    const grauNormalizado = valor.toLowerCase();\n    if (grauNormalizado.includes('bacharel')) dados.grau = 'Bacharelado';\n    else if (grauNormalizado.includes('licenciatura')) dados.grau = 'Licenciatura';\n    else if (grauNormalizado.includes('tecnólogo') || grauNormalizado.includes('tecnologo')) dados.grau = 'Tecnólogo';\n    else dados.grau = valor.charAt(0).toUpperCase() + valor.slice(1).toLowerCase(); // Primeira letra maiúscula\n  }\n});\nreturn [{\n  json: {\n    ...dados,\n  }\n}];"
      },
