    {
      "parameters": {
        "jsCode": "const inputData = items[0].json._embedded.leads;\n\nconst results = inputData.map(lead => {\n  const campos = {};\n\n  // Mapeia os campos personalizados\n  if (lead.custom_fields_values) {\n    lead.custom_fields_values.forEach(field => {\n      campos[field.field_name] = {\n        field_id: field.field_id,\n        field_type: field.field_type,\n        values: field.values ?? [],\n        raw: field\n      };\n    });\n  }\n\n  // ðŸ“… Converte a matrÃ­cula de timestamp para DD/MM/YYYY ou \"\" se invÃ¡lida\n  let matriculaFormatada = \"\";\n  const matriculaRaw = campos[\"MatrÃ­cula\"]?.raw?.values?.[0]?.value;\n\n  if (matriculaRaw && !isNaN(matriculaRaw)) {\n    const timestamp = Number(matriculaRaw);\n    const date = new Date(timestamp * 1000); // Unix timestamp\n    const brasiliaDate = new Date(date.getTime() - 3 * 60 * 60 * 1000); // Ajuste GMT-3\n\n    const pad = n => n.toString().padStart(2, '0');\n    matriculaFormatada = `${pad(brasiliaDate.getDate())}/${pad(brasiliaDate.getMonth() + 1)}/${brasiliaDate.getFullYear()}`;\n  }\n\n  return {\n    json: {\n      id: lead.id ?? \"\",\n      name: lead.name ?? \"\",\n      pipeline_id: lead.pipeline_id ?? \"\",\n      campos,\n      telefone: campos[\"Telefone Comercial\"]?.values?.[0]?.value ?? \"\",\n      email: campos[\"E-mail\"]?.values?.[0]?.value ?? \"\",\n      matriculaFormatada\n    }\n  };\n});\n\nreturn results;\n"
      },
